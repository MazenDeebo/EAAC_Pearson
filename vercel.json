{
  "version": 2,
  "name": "eaac-pearson-btec",
  "builds": [
    {
      "src": "**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/business",
      "dest": "/pages/business-management.html"
    },
    {
      "src": "/it",
      "dest": "/pages/it-computing.html"
    },
    {
      "src": "/ai",
      "dest": "/pages/artificial-intelligence.html"
    },
    {
      "src": "/engineering",
      "dest": "/pages/engineering.html"
    }
  ],
  "headers": [
    {
      "source": "/assets/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}
